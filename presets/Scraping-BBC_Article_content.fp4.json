{
  "name": "BBC: Article content",
  "category": "Scraping",
  "description": "Add URL to a BBC article as seed node. You can get URLs with the search preset. This preset extracts the text blocks from the page\n\nSee the wiki for information about using CSS and XPath selectors with Facepager: https://github.com/strohne/Facepager/wiki/Webscraping",
  "module": "Generic",
  "options": {
    "auth": "disable",
    "auth_prefix": "",
    "auth_tokenname": "",
    "auth_type": "Disable",
    "auth_uri": "",
    "basepath": "<Object ID>",
    "extension": "",
    "fileext": "<None>",
    "filename": "<None>",
    "format": "text",
    "headers": {},
    "nodedata": "text|xpath://div[@data-component=\"text-block\"]",
    "objectid": "text|xpath:string()",
    "offset_start": 1,
    "offset_step": 1,
    "pages": 1,
    "paging_type": "count",
    "param_paging": null,
    "params": {},
    "proxy": "",
    "redirect_uri": "",
    "resource": "",
    "scope": "",
    "token_uri": "",
    "verb": "GET"
  },
  "speed": 200,
  "saveheaders": false,
  "timeout": 15,
  "columns": []
}